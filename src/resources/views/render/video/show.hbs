{{#with videos}}
<div class="mt-4 grid wide">
    <div class="row">
        <div class="col-3">
            <button class="p-1">
                <h5 style="line-height: 100%;" class="d-block">Đăng ký ngay!</h5>
            </button>
        </div>
        <div class="col-9">
            <h2>{{name}}</h2>
            <iframe style="max-width: 700px;" width="100%" height="350px"
                src="https://www.youtube.com/embed/{{videoId}}" title="YouTube video player" frameborder="0"
                allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
                allowfullscreen>
            </iframe>
            <p>{{decription}}</p>
        </div>
    </div>
</div>
{{/with}}